
import java.util.Scanner;
 
class VotersAge
{
   public static void main(String args[])
   {
      
	   int a;
	   Scanner inputage= new Scanner(System.in);
 
 
      System.out.println("Enter your age ");
      a = inputage.nextInt();
      
      
      if (a <= 17)
  		System.out.println("A person who is \""  +a + "\" years old is not allowed to vote");  		
  		else
  		System.out.println("A person who is \""   +a  + "\" years old is allowed to vote");
      
    
   }
}
