import java.util.Scanner;

public class SquareRoot {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner scan = new Scanner(System.in);

		int sqrtof = 0;

		System.out.println("Put the number you want the Square Root of: ");
		sqrtof = scan.nextInt();

		System.out.println("The square root of " + sqrtof + " is " + Math.sqrt(sqrtof));

	}

}
